{
	"name": "node-shell-support",
	"version": "1.0.0",
	"description": "Classes designed to run various command line programs under Linux or MacOSX",
	"main": "lib/index.js",
	"author": "Russell Weir",
	"license": "MIT",
	"dependencies": {},
	"devDependencies": {
		"@types/node": "^14.11.7",
		"typedoc": "^0.19.2",
		"typedoc-plugin-markdown": "^3.0.7",
		"typescript": "^4.0.3"
	},
	"scripts": {
		"build": "tsc -b tsconfig.json",
		"doc": "npx typedoc --plugin typedoc-plugin-markdown --tsconfig typedoc.json",
		"doc-developer": "npx typedoc --tsconfig typedoc-developer.json",
		"release": "npm run build && npm run doc"
	}
}
